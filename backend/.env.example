# Example env file for Face Auth MVP
# Copy to `.env` and fill in real values before running.

# Similarity threshold for passing face match
SIM_THRESHOLD=0.6

# Device to use for PyTorch (cpu, mps, cuda)
DEVICE=cpu

# Optional: OpenMP runtime settings for macOS
OMP_NUM_THREADS=1
MKL_NUM_THREADS=1
KMP_DUPLICATE_LIB_OK=TRUE
